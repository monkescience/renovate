{
  "name": "branch-naming",
  "target": "branch",
  "enforcement": "active",
  "conditions": {
    "ref_name": {
      "exclude": [
        "~DEFAULT_BRANCH",
        "refs/heads/renovate/*",
        "refs/heads/release-please--*"
      ],
      "include": [
        "~ALL"
      ]
    }
  },
  "rules": [
    {
      "type": "branch_name_pattern",
      "parameters": {
        "operator": "regex",
        "pattern": "^(feat|fix|chore|docs|refactor|test|ci|build|perf|style|revert)/[a-z0-9-]+$",
        "negate": false,
        "name": "Conventional Branch Names"
      }
    }
  ],
  "bypass_actors": []
}
